{
	"name": "VTOP-assignment-callendar",
	"version": "1.0",
	"permissions": [
		"contextMenus",
		"declarativeContent",
		"storage",
		"activeTab"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/icon16.png",
			"32": "images/icon32.png",
			"48": "images/icon48.png",
			"128": "images/icon128.png"
		}
	},
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"http://vtop.vit.ac.in/*"
			],
			"js": [
				"contentScript.js"
			]
		}
	],
	"description": "Add your assignments to your callendar",
	"manifest_version": 2
}